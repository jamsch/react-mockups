{
  "name": "@jamsch/react-mockups",
  "version": "0.5.4",
  "description": "Tools to develop components in isolation for React & React Native",
  "main": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    "./native": {
      "require": "./native.js",
      "import": "./native.mjs"
    },
    "./web": {
      "require": "./web.js",
      "import": "./web.mjs"
    }
  },
  "bin": {
    "react-mockups": "./cli.js"
  },
  "files": [
    "src",
    "lib",
    "index.js",
    "index.d.ts",
    "web.js",
    "web.d.ts",
    "web.mjs",
    "native.js",
    "native.d.ts",
    "native.mjs",
    "cli.js"
  ],
  "scripts": {
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "rollup -c && husky install",
    "release": "release-it",
    "example": "yarn --cwd example"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/jamsch/react-mockups",
  "author": "jamsch <jamsch18@student.wintec.ac.nz> (https://github.com/jamsch)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jamsch/react-mockups/issues"
  },
  "homepage": "https://github.com/jamsch/react-mockups#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "glob": "^7.2.0",
    "prettier": "^2.5.1",
    "ws": "^8.3.0",
    "yargs": "^17.3.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^15.0.0",
    "@react-native-community/eslint-config": "^3.0.1",
    "@release-it/conventional-changelog": "^3.3.0",
    "@types/glob": "^7.2.0",
    "@types/prettier": "^2.4.2",
    "@types/react": "^17.0.37",
    "@types/react-native": "0.66.8",
    "@types/ws": "^8.2.2",
    "commitlint": "^15.0.0",
    "esbuild": "^0.14.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "react": "^17.0.1",
    "react-native": "0.64.3",
    "release-it": "^14.11.8",
    "rollup": "^2.60.2",
    "rollup-plugin-dts": "^4.0.1",
    "rollup-plugin-esbuild": "^4.7.2",
    "rollup-plugin-preserve-shebang": "^1.0.1",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {
    "react": ">=16.10.0",
    "react-native": ">=0.61.0"
  },
  "peerDependenciesMeta": {
    "react-native": {
      "optional": true
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "globals": {
      "NodeRequire": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "cli.js",
    "index.js",
    "index.d.ts",
    "native.js",
    "native.d.ts",
    "native.mjs",
    "web.js",
    "web.d.ts",
    "web.mjs",
    "node_modules/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  }
}
