{
  "name": "react-mockups-example",
  "description": "Example app for react-mockups using React Native + Expo",
  "version": "0.0.1",
  "private": true,
  "main": "index",
  "scripts": {
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "start": "expo start",
    "start:mockups": "npm-run-all --parallel mockup-server start",
    "mockup-server": "react-mockups server",
    "premockup-server": "node ./adb-reverse.js && react-mockups generate"
  },
  "config": {
    "react-mockups": {
      "host": "localhost",
      "port": "1337",
      "searchDir": [
        "./src"
      ],
      "pattern": "**/*.mockup.tsx",
      "outputFile": "./src/mockups.ts"
    }
  },
  "dependencies": {
    "@jamsch/react-mockups": "latest",
    "@react-native-async-storage/async-storage": "^1.15.14",
    "expo": "^43.0.3",
    "expo-splash-screen": "~0.13.5",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-native": "0.64.3"
  },
  "devDependencies": {
    "@babel/core": "~7.16.0",
    "@babel/runtime": "^7.16.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-preset-expo": "8.5.1",
    "expo-cli": "^4.13.0"
  }
}
